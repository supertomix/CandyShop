@model ShopCartViewModel
<div class="container">
    @foreach(var el in Model.shopCart.listShopItems)
    {
    <div class="alert alert-warning mt-3">
        <b>Товар:</b>@el.product.Name<br />
        <b>Стоимость:</b>@el.product.Prise<br />
    </div>
    }
    <hr/>
    <a class="btn btn-danger" asp-controller="Order" asp-action="Checkout">В корзину</a>

</div> 